<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:f-rin="http://ddex.net/xml/f-rin/11" xmlns:avs="http://ddex.net/xml/avs/avs"
   xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:xs="http://www.w3.org/2001/XMLSchema"
   targetNamespace="http://ddex.net/xml/f-rin/11" elementFormDefault="unqualified"
   attributeFormDefault="unqualified">
   <xs:import namespace="http://ddex.net/xml/avs/avs" schemaLocation="avs4.xsd"/>
   <xs:import namespace="http://www.w3.org/2000/09/xmldsig#"
      schemaLocation="http://www.w3.org/TR/2002/REC-xmldsig-core-20020212/xmldsig-core-schema.xsd"/>
   <xs:annotation>
      <xs:documentation>Â© 2006-2018 Digital Data Exchange, LLC (DDEX)</xs:documentation>
