   </xs:annotation>
   <xs:element name="RecordingInformationNotification">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A File in the DDEX Recording Information Notification Standard, containing information about one or more Recordings as well as related entries such as involved Parties, Sessions, RecordingComponents.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="FileHeader" type="f-rin:FileHeader">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">The file header for the RecordingInformationNotification.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="Comment" minOccurs="0" maxOccurs="unbounded" type="f-rin:Comment">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing a human-readable Comment about the Message.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="PartyList" minOccurs="0" type="f-rin:PartyList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more Parties relating to the reported MusicalWorks.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="MusicalWorkList" minOccurs="0" type="f-rin:MusicalWorkList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more MusicalWorks.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="ResourceList" minOccurs="0" type="f-rin:ResourceList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more Resources.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="RecordingComponentList" minOccurs="0"
               type="f-rin:RecordingComponentList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more RecordingComponents.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="ProjectList" minOccurs="0" type="f-rin:ProjectList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more Projects.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="SessionList" minOccurs="0" type="f-rin:SessionList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more Sessions.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="EquipmentList" minOccurs="0" type="f-rin:EquipmentList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of Equipment.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="DataCarrierList" minOccurs="0" type="f-rin:DataCarrierList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more DataCarriers.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="ElementList" minOccurs="0" type="f-rin:ElementList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more Elements.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="FileList" minOccurs="0" type="f-rin:FileList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more Files.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="TransferList" minOccurs="0" type="f-rin:TransferList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more Transfers.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="ImportedRinFiles" minOccurs="0" type="f-rin:ImportedRinFiles">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more Files incorporated into the RecordingInformationNotification.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="SchemaVersionId" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Identifier of the Version of the XML schema used for the File. This is represented in an XML schema as an XML Attribute.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="LanguageAndScriptCode" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Language and script for the Elements of the RecordingInformationNotification as defined in IETF RfC 5646. Language and Script are provided as lang[-script][-region][-variant]. This is represented in an XML schema as an XML Attribute.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:complexType name="DataCarrier">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a DataCarrier.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="DataCarrierId" minOccurs="0" maxOccurs="unbounded"
            type="f-rin:DataCarrierId">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of an Identifier of the DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DataCarrierReference">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Identifier (specific to the File) of the DataCarrier. This is a LocalDataCarrierAnchor starting with the letter D.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:ID">
                  <xs:pattern value="D[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="DataCarrierType" type="avs:DataCarrierType">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Type of DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Title" minOccurs="0" maxOccurs="unbounded" type="f-rin:Title">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Title of the DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ManufacturerName" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Name of the Party manufacturing the DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ModelName" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Name of the model.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SerialNumber" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The serial number of the DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NumberOfDataCarriersInSet" minOccurs="0" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The total number of DataCarriers in a multi-DataCarrier set.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DataCarrierOfSet" minOccurs="0" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The DataCarrier item number in a multi-DataCarrier set.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ContentSummary" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A summary of the contents of the DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CurrentLocation" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Description of the current location of the DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Comment" minOccurs="0" type="f-rin:Comment">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing a human-readable Comment about the DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DataCarrierProjectReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Project (specific to the File). This is a LocalProjectAnchorReference starting with the letter J.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="J[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="DataCarrierFileReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a File stored on the DataCarrier (specific to this Message). This is a LocalFileAnchorReference starting with the letter F.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="F[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="DataCarrierContent" minOccurs="0" maxOccurs="unbounded"
            type="f-rin:DataCarrierContent">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of items located on the DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DataCarrierContent">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of items located on a DataCarrier.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="DataCarrierContentItem" maxOccurs="unbounded"
            type="f-rin:DataCarrierContentItem">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DataCarrierContentItem">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of an item located on a DataCarrier.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="SequenceNumber" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The number indicating the order of the ContentItem in a group of ContentItems within a DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Description" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A textual Description of the ContentItem.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Comment" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An additional Comment about the ContentItem.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DataCarrierId">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of DataCarrierIds.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ProprietaryId" minOccurs="0" maxOccurs="unbounded"
            type="f-rin:ProprietaryId">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a ProprietaryIdentifier of the DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DataCarrierList">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of one or more DataCarriers.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="DataCarrier" maxOccurs="unbounded" type="f-rin:DataCarrier">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="LanguageAndScriptCode" type="xs:string">
         <xs:annotation>
            <xs:documentation source="ddex:Definition">The Language and script for the Elements of the DataCarrierList as defined in IETF RfC 5646. The default is the same as indicated for the containing composite. Language and Script are provided as lang[-script][-region][-variant]. This is represented in an XML schema as an XML Attribute.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="Details">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing further details of a Transfer.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="SequenceNumber" minOccurs="0" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The number indicating the order of this Details composite in a group of composites.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="TransferCategory" type="avs:TransferCategory">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The category pertaining to the details of the Transfer.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Description" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Description of the details for the TransferCategory.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Comment" minOccurs="0" type="f-rin:Comment">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing a further details of the Description.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DisplayArtist">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a DisplayArtist of a Resource.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="PartyReference">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message). This is a LocalPartyAnchorReference starting with the letter P.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="P[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="TitleDisplayInformation" minOccurs="0" maxOccurs="unbounded"
            type="f-rin:TitleDisplayInformation">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing information on how a RecordCompany wishes Artist information to be presented to Consumers as part of the Title (and in addition to displaying the DisplayArtist information).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ArtisticRole" maxOccurs="unbounded" type="f-rin:ContributorRole">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a ContributorRole played by the DisplayArtist.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="SequenceNumber" type="xs:integer">
         <xs:annotation>
            <xs:documentation source="ddex:Definition">The number indicating the order of the Resource DisplayArtist in a group of Artists that have contributed to a Resource. This is represented in an XML schema as an XML Attribute. </xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="Element">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a recording Element.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ElementId" minOccurs="0" maxOccurs="unbounded" type="f-rin:ElementId">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of an Identifier of the Element.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ElementReference">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Identifier (specific to the File) of the Element. This is a LocalElementAnchor starting with the letter M.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:ID">
                  <xs:pattern value="M[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="Title" minOccurs="0" maxOccurs="unbounded" type="f-rin:Title">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Title of the Element.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="RightsController" minOccurs="0" maxOccurs="unbounded"
            type="f-rin:RightsController">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of RightsController of Rights in the Element.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IsMultiTrack" minOccurs="0" type="xs:boolean">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Flag indicating whether the Element is a multitrack Element (=true) or not (=false).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Designation" minOccurs="0" type="avs:ElementDesignation">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of the Element based on its designation.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Configuration" minOccurs="0" type="avs:ElementConfiguration">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of the Element based on its configuration.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="BitDepth" minOccurs="0" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The BitDepth of the Element.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SamplingRate" minOccurs="0" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The sample rate of the Element in kHz.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Timecode" minOccurs="0" type="f-rin:Timecode">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing the time code of the Element.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="FileType" minOccurs="0" type="avs:FileType">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Type of File.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PLine" minOccurs="0" type="f-rin:PLineWithDefault">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the PLine for the Element.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ParentalWarningType" minOccurs="0"
            type="f-rin:ParentalWarningTypeWithTerritory">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the classification of the Element according to advice which it carries about the level of explicitness or offensiveness of its content.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Genre" minOccurs="0" maxOccurs="unbounded"
            type="f-rin:GenreWithTerritory">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Genre to which the Element belongs.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Status" minOccurs="0" type="f-rin:Status">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Status of the Element.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Comment" minOccurs="0" type="f-rin:Comment">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing a human-readable Comment about the Element.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ElementDataCarrierReference" type="f-rin:ElementDataCarrierReference">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a DataCarrier in which the Element is used.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ElementSessionReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Session (specific to the File). This is a LocalSessionAnchorReference starting with the letter O.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="O[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="ElementSoundRecordingReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a SoundRecording for the Element (specific to this Message). This is a LocalResourceAnchorReference starting with the letter A.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="A[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ElementDataCarrierReference">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a DataCarrier in which an Element is used.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ElementDataCarrierReference" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a DataCarrier in which an Element is used (specific to this Message). This is a LocalDataCarrierAnchorReference starting with the letter D.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="D[\d\-_a-zA-Z]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="DataCarrierFormat" type="avs:DataCarrierFormat">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The format of the DataCarrier.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ElementId">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of ElementIds.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ProprietaryId" minOccurs="0" maxOccurs="unbounded"
            type="f-rin:ProprietaryId">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a ProprietaryIdentifier of the Element.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ElementList">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of one or more Elements.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Element" maxOccurs="unbounded" type="f-rin:Element">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Element.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="LanguageAndScriptCode" type="xs:string">
         <xs:annotation>
            <xs:documentation source="ddex:Definition">The Language and script for the Elements of the ElementList as defined in IETF RfC 5646. The default is the same as indicated for the containing composite. Language and Script are provided as lang[-script][-region][-variant]. This is represented in an XML schema as an XML Attribute.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="Equipment">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a piece of Equipment.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="EquipmentId" minOccurs="0" maxOccurs="unbounded" type="f-rin:EquipmentId">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of an Identifier of the Equipment.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="EquipmentReference">
