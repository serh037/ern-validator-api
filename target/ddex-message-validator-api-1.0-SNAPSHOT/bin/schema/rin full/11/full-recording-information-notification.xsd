         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="PLineWithDefault">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a PLine.</xs:documentation>
      </xs:annotation>
